{
  "expo": {
      "name": "Start JÃ¡",
      "slug": "startja",
      "owner": "agenciaboz-dev",
      "scheme": "startja",
      "version": "1.1.4",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
          "image": "./assets/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
          "userInterfaceStyle": "light",
          "supportsTablet": false,
          "bundleIdentifier": "com.agenciabozdev.startja",
          "associatedDomains": ["applinks:startja.agenciaboz.com.br"]
      },
      "android": {
          "userInterfaceStyle": "light",
          "adaptiveIcon": {
              "foregroundImage": "./assets/foreground-icon.png",
              "monochromeImage": "./assets/adaptive-icon.png",
              "backgroundColor": "#ffffff"
          },
          "softwareKeyboardLayoutMode": "resize",
          "package": "com.agenciabozdev.startja",
          "permissions": ["android.permission.RECORD_AUDIO"],
          "googleServicesFile": "./google-services.json"
      },
      "web": {
          "favicon": "./assets/favicon.png"
      },
      "notification": {
          "icon": "./assets/notification-icon.png",
          "color": "#007500"
      },
      "plugins": [
          ["./plugins/withDisableForcedDarkModeAndroid.js", {}],
          ["./plugins/withAndroidLargeHeap.js", {}],
          [
              "expo-image-picker",
              {
                  "photosPermission": "The app accesses your photos to let you upload it in your profile."
              }
          ],
          [
              "expo-font",
              {
                  "fonts": []
              }
          ],
          [
              "expo-notifications",
              {
                  "icon": "./assets/notification-icon.png",
                  "color": "#007500"
              }
          ]
      ],
      "extra": {
          "eas": {
              "projectId": "b46f6f2d-43a6-41c2-afc2-9429efdd4e6e"
          }
      }
  }
}
