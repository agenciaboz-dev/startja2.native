{
    "expo": {
        "name": "Start JÃ¡",
        "slug": "startja",
        "owner": "agenciaboz-dev",
        "scheme": "startja",
        "version": "1.1.3",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#000"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "userInterfaceStyle": "light",
            "supportsTablet": false,
            "bundleIdentifier": "com.agenciabozdev.startja",
            "associatedDomains": ["applinks:startja.agenciaboz.com.br"]
        },
        "android": {
            "userInterfaceStyle": "light",
            "adaptiveIcon": {
                "foregroundImage": "./assets/foreground-icon.png",
                "monochromeImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#000"
            },
            "softwareKeyboardLayoutMode": "resize",
            "package": "com.agenciabozdev.startja"
            // "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "notification": {
            "icon": "./assets/notification-icon.png",
            "color": "#000000"
        },
        "plugins": [
            ["./plugins/withDisableForcedDarkModeAndroid.js", {}],
            ["./plugins/withAndroidLargeHeap.js", {}],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you upload it in your profile."
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        // "assets/fonts/custom/FoundersGroteskBOZ-Regular.otf",
                    ]
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification-icon.png",
                    "color": "#000000"
                }
            ]
        ]
    }
}
  